                            <div id="errort1" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="t2" class="label-form control-label lbl-black">Username: </label>
                            <input type="text" id="t2" class="ag-control input-sm ctrl-width" name="UserName" data-rule-required="true" aria-required="true">
                            <div id="errort2" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="t3" class="label-form control-label lbl-black">Password: </label>
                            <input type="text" id="t3" class="ag-control input-sm ctrl-width" name="Password" data-rule-required="true" aria-required="true" value="@Model.TempPswd">
                            <div id="errort3" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="t4" class="label-form control-label lbl-black">First Name: </label>
                            <input type="text" id="t4" class="ag-control input-sm ctrl-width" name="FirstName" data-rule-required="true" aria-required="true">
                            <div id="errort4" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="t5" class="label-form control-label lbl-black">Last Name: </label>
                            <input type="text" id="t5" class="ag-control input-sm ctrl-width" name="LastName" data-rule-required="true" aria-required="true">
                            <div id="errort5" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="t6" class="label-form control-label lbl-black">Address: </label>
                            <input type="text" id="t6" class="ag-control input-sm ctrl-width" name="Address" data-rule-required="true" aria-required="true">
                            <div id="errort6" class="has-error pad-err"></div>
                        </div>
