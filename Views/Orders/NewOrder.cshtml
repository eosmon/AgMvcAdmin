                    </div>
                    <div class="panel-right">
                        <div class="top-buffer">
                            <label for="t8" class="label-form control-label lbl-black pad-lft">City: </label><input type="text" id="t8" class="ag-control input-sm ctrl-width" name="City" data-rule-required="true" aria-required="true">
                            <div id="errort8" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="s2" class="label-form control-label lbl-black pad-lft">State: </label>
                            @Html.DropDownList("StateId", Model.Menus.UsStateNames.UsStates, "-STATE- ", new { id = "s2", @class = "ag-control input-sm ctrl-width", style = "margin-left:-3px", onchange = "reqCaDocs()" })
