                                <select id="s14" class="ag-control input-sm ctrl-width" name="CaHasGunSafe" data-rule-required="true" aria-required="true">
                                    <option value="" selected>- SELECT -</option>
                                    <option value="true">YES</option>
                                    <option value="false">NO</option>
                                </select>
                                <div id="errors14" class="has-error pad-err"></div>
