                        </div>
                        <div class="top-buffer">
                            <label for="s3" class="label-form control-label lbl-black">Referred By: </label>
                            @Html.DropDownList("SourceId", Model.Menus.RefModel.ReferralSources, "- SELECT - ", new { id = "s3", @class = "ag-control input-sm ctrl-width" })
                            <div id="errors3" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="s28" class="label-form control-label lbl-black">VIP Customer: </label>
                            <select id="s28" class="ag-control input-sm ctrl-width" name="PreferredCustomer" data-rule-required="true" aria-required="true">
                                <option value="" selected>- SELECT -</option>
                                <option value="true">Yes</option>
                                <option value="false">No</option>
                            </select>
                            <div id="errors28" class="has-error pad-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="s4" class="label-form control-label lbl-black">Profession: </label>
                            <select name="ProfessionId" id="s4" class="selectpicker form-group-sm ctrl-width" data-live-search="true" data-size="10" data-hide-disabled="true" data-rule-required="true" aria-required="true">
                                @{
                                    <option>- SELECT -</option>
                                    <option value="-1">- ADD NEW PROFESSION -</option>
                                    foreach (var item in Model.Menus.CustProfession.Professions)
                                    {
                                        <option value="@item.Value">@item.Text</option>
                                    }
                                }
                            </select>
                            <div id="errors4" class="has-error pad-err"></div>
                        </div>
                    </div>
                </form>                
            </div>
            <div style="clear: both; height: 10px"></div>
