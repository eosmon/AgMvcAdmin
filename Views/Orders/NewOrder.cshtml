            @{
                <option>- SELECT -</option>
                <option value="-1">- ADD NEW CUSTOMER -</option>
                foreach (var item in Model.Menus.AddCustomer.Customers)
                {
                    <option value="@item.Value">@item.Text</option>
                }
            }
            </select>
            <div id="errors1" class="has-error pad-err"></div>
        </div>
        <div id="divAddCustomer">
            <div style="clear: both; height: 10px"></div>
            <div class="well-grp">Add New Customer</div>
            <div class="well-text row" style="margin-left: 0; width: 100%">
