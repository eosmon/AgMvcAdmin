                            @Html.DropDownList("DobYear", Model.Menus.YrModel.Year, "-YR- ", new { id = "cs37", @class = "ag-control-short input-sm", style = "width:60px; margin-left: 1px;" })
                        </div>
                        <div id="errorcs35" class="has-error cust-err" style="float:left"></div>
                        <div id="errorcs36" class="has-error cust-err" style="float:left"></div>
                        <div id="errorcs37" class="has-error cust-err" style="float:left"></div>
                    </div>
                </div>
                <div class="pnl-right">

                </div>
            </div>
            <div id="pnlCustPvtBiz" style="display: none">
