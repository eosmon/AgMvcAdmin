@model AgMvcAdmin.Models.PageModel

<div id="dvCustBasic" style="display:none">
    <form data-form-validate="true" action="" method="post" id="cust-basic-info" novalidate="novalidate">
        <div class="well-grp">Customer Basic Info</div>
        <div class="well-text row" style="width: 100%; margin: 0 auto">
            <div>
                <div class="pnl-left">
                    <div class="top-buffer">
                        <label for="ct1" class="label-form lbl-black">First Name: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="FirstName" id="ct1" data-rule-required="true" aria-required="true">
                        <div id="errorct1" class="has-error cust-edit-pad"></div>
                    </div>
                    <div class="top-buffer">
                        <label for="ct2" class="label-form lbl-black">Last Name: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="LastName" id="ct2" data-rule-required="true" aria-required="true">
                        <div id="errorct2" class="has-error cust-edit-pad"></div>
                    </div>
                </div>
                <div class="pnl-right">
                    <div style="padding-top: 4px">
                        <label for="ct9" class="label-form lbl-black">Email Address: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft req-group" name="EmailAddress" id="ct9">
                        <div id="errorct9" class="has-error cust-edit-pad"></div>
                    </div>
                    <div style="padding-top: 4px">
                        <label for="ct10" class="label-form lbl-black">Phone: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft req-group" name="Phone" id="ct10">
                        <div id="errorct10" class="has-error cust-edit-pad"></div>
                    </div>
                </div>
                <div style="clear: both; padding-top: 4px">
                    <label for="cta1" class="label-form lbl-black" style="vertical-align: top; padding-top: 5px">Notes: </label><textarea id="cta1" class="ag-control input-sm ctrl-width pad-lft" name="CustomerNotes" style="height: 94px"></textarea>
                    <div id="errorcta1" class="has-error cust-edit-pad"></div>
                </div>
            </div>
        </div>
    </form>
    <div style="clear: both; height: 25px"></div>
    <div id="dvCustProfile">
        <div class="well-grp">Customer Profile</div>
        <div class="well-text row" style="width: 100%; margin: 0 auto">
            <form data-form-validate="true" action="" method="post" id="cust-form-profile" novalidate="novalidate">
                <div class="pnl-left">
                    <div class="top-buffer">
                        <label for="cs16" class="label-form lbl-black">VIP Customer: </label>
                        <select name="IsVip" id="cs16" class="ag-control input-sm ctrl-width">
                            <option value="0">- SELECT -</option>
                            <option value="true">Yes</option>
                            <option value="false">No</option>
                        </select>
                        <div id="errorcs16" class="has-error cust-err"></div>
                    </div>
                    <div class="top-buffer">
                        <label for="cs2" class="label-form lbl-black">Referred By: </label>
                        <select name="SourceId" id="cs2" class="ag-control input-sm ctrl-width">
                            @{
                                <option value="">- SELECT -</option>
                                foreach (var item in Model.Menus.RefModel.ReferralSources)
                                {
                                    <option value="@item.Value">@item.Text</option>
                                }
                            }
                        </select>
                        <div id="errorcs2" class="has-error cust-err"></div>
                    </div>
                    <div class="top-buffer">
                        <label for="cs13" class="label-form lbl-black">Select Address: </label>
                        <select name="CustAddressID" id="cs13" class="ag-control input-sm ctrl-width" onchange="setCustAddr(this.value)">
                            <option value="">- SELECT -</option>
                            <option value="-1">- ADD NEW ADDRESS -</option>
                        </select>
                        <div id="errorcs13" class="has-error cust-err"></div>
                    </div>
                    <div id="dvAddr" style="display:none">
                        <div class="top-buffer">
                            <label for="ct4" class="label-form lbl-black">Address: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="Address" id="ct4">
                            <div id="errorct4" class="has-error cust-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="ct5" class="label-form lbl-black">Suite: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="Suite" id="ct5">
                            <div id="errorct5" class="has-error cust-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="ct6" class="label-form lbl-black">City: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="City" id="ct6">
                            <div id="errorct6" class="has-error cust-err"></div>
                        </div>
                        <div class="top-buffer">
                            <label for="cs3" class="label-form lbl-black state-label">State: </label>
                            <select name="StateId" id="cs3" class="ag-control-short input-sm" style="width: 55px; margin-left: 6px; float:left" onchange="showCt(this.value)">
                                @{
                                    <option value="0">-ST- </option>
                                    foreach (var item in Model.Menus.UsStateAbrv.UsStates)
                                    {
                                        <option value="@item.Value">@item.Text</option>
                                    }
                                }
                            </select>
                            <div style="float:right; margin-right:3px">
                                <label id="l2" for="ct7" class="control-label  lbl-black">Zip Code: </label>
                                <div class="zip-wrap">
