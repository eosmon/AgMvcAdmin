                                    @Html.DropDownList("CustFscDay", Model.Menus.DdModel.Day, "-DD- ", new { id = "cs39", @class = "ag-control-short input-sm", style = "width:60px; margin-left: 1px;" })
                                    @Html.DropDownList("CustFscYear", Model.Menus.YrModel.Year, "-YR- ", new { id = "cs40", @class = "ag-control-short input-sm", style = "width:60px; margin-left: 1px;" })
                                </div>
