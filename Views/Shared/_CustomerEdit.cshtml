                                <input type="text" id="ct30" class="ag-control input-sm ctrl-width" name="FscNumber" data-rule-required="true" aria-required="true">
                                <div id="errorct30" class="has-error cust-err"></div>
                            </div>
                            <div class="top-buffer" style="text-align:left">
