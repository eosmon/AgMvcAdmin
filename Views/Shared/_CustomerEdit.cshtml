                                    <div id="errorct7" class="has-error"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="pnl-right">
                    <div>
                        <label for="ct28" class="label-form lbl-black">FAX #: </label>
                        <input type="text" id="ct28" class="ag-control input-sm ctrl-width" name="Fax" aria-required="true" placeholder="Type Numbers Only">
                        <div id="errorct28" class="has-error cust-err"></div>
                    </div>
                    <div class="top-buffer">
                        <label for="cs4" class="label-form lbl-black">Industry: </label>
                        <select name="IndustryId" id="cs4" class="ag-control input-sm ctrl-width" onchange="showAddMenuItem(1)">
                            @{
                                <option value="0">- SELECT -</option>
                                <option value="-1">ADD NEW INDUSTRY</option>
                                foreach (var item in Model.Menus.Industry)
                                {
                                    <option value="@item.Value">@item.Text</option>
                                }
                                <option value="-2">DECLINE TO SAY</option>
                            }
                        </select>
                        <div id="errorcs4" class="has-error cust-err"></div>
                    </div>
                    <div id="addIndustry" style="display: none">
                        <div class="top-buffer">
                            <label for="ct11" class="label-form lbl-black">New Industry: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="IndustryAdd" id="t11">
                            <div id="errorsct11" class="has-error cust-err"></div>
                        </div>
                        <div class="top-buffer" style="text-align: right; margin-right: 7px">
                            <button type="button" class="btn btn-xs btn-blue btn-inv-guns" id="btnAddIndustry" onclick="addIndustry()">Add Industry</button>
                        </div>
                    </div>
                    <div class="top-buffer" id="dvCustProfession" style="display: none">
                        <label for="cs5" class="label-form lbl-black">Profession: </label>
                        <select name="ProfessionId" id="cs5" class="ag-control input-sm ctrl-width" onchange="showAddMenuItem(2)">
                            <option value="0">- SELECT -</option>
                            <option value="-1">ADD NEW PROFESSION</option>
                            <option value="-2">DECLINE TO SAY</option>
                        </select>
                        <div id="errorcs5" class="has-error cust-err"></div>
                    </div>
                    <div id="addProfession" style="display: none">
                        <div class="top-buffer">
                            <label for="ct12" class="label-form lbl-black">New Profession: </label><input type="text" class="ag-control input-sm ctrl-width pad-lft" name="IndustryAdd" id="ct12">
                            <div id="errorsct12" class="has-error cust-err"></div>
                        </div>
                        <div class="top-buffer" style="text-align: right; margin-right: 7px">
                            <button type="button" class="btn btn-xs btn-blue btn-inv-guns" id="btnAddProfession" onclick="addProfession()">Add Profession</button>
                        </div>
                    </div>
                    <div class="top-buffer">
                        <label for="cs17" class="label-form lbl-black">Website Access: </label>
                        <select name="IsOnWeb" id="cs17" class="ag-control input-sm ctrl-width" onchange="custWebAccess(this.value)">
                            <option value="">- SELECT -</option>
                            <option value="true">Yes</option>
                            <option value="false">No</option>
                        </select>
                        <div id="errorcs16" class="has-error cust-err"></div>
                    </div>
                    <div>
                        <label for="ProfileImg" class="label-form lbl-black" style="vertical-align: top; padding-top: 8px">Profile Pic: </label>
                        <div class="fileinput fileinput-new img-loop" data-provides="fileinput" style="display: inline-block; width:calc(100% - 115px); margin-bottom:0 !important; padding-top:10px">
                            <div id="CusImg_1" class="fileinput-preview thumbnail cust-profile-pic" data-trigger="fileinput" style="min-height:90px"></div>
                            <div class="checkForm" style="text-align: center; display: inline-block; padding-left: 5px;">
                                <span class="btn-file"><a href="#" class="fileinput-new uploadTxt" data-trigger="fileinput">Select Image</a><input type="file" name="agimg" id="ProfileImg"></span>
                                <a href="#" class="fileinput-exists uploadTxt img-loop" data-dismiss="fileinput">Remove</a>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <div id="dvCustCt" style="float:left; width:100%">
                <div class="pnl-left">
                    <div class="top-buffer" id="dvCustType">
                        <label for="cs1" class="lbl-black" style="width:110px; text-align:right">Customer Type: </label>
                        <select name="CustomerTypeId" id="cs1" class="ag-control input-sm ctrl-width" onchange="setPanel(this.value)">
                            @{
                                <option value="0">-SELECT-</option>
                                foreach (var item in Model.Menus.CustTypes.CustomerTypes)
                                {
                                    <option value="@item.Value">@item.Text</option>
                                }
                            }
                        </select>
                        <div id="errorcs1" class="has-error cust-err"></div>
                    </div>
                    <div class="top-buffer" id="dvDob" style="display:none; text-align:left">
                        <label for="cs35" class="lbl-black" style="width:114px; text-align:right">Date of Birth: </label>
                        <div class="date-wrap" style="margin-left:-2px">
                            @Html.DropDownList("DobMonth", Model.Menus.MoModel.Month, "-MO- ", new { id = "cs35", @class = "ag-control-short input-sm", style = "width:60px; margin-left: 1px;" })
